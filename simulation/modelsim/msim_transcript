# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.detector_paridade(behavior)
add wave -position end  sim:/detector_paridade/bits
add wave -position end  sim:/detector_paridade/dados_in
add wave -position end  sim:/detector_paridade/im_par
add wave -position end  sim:/detector_paridade/parity
add wave -position end  sim:/detector_paridade/output
force -freeze sim:/detector_paridade/dados_in 0101101001 0
force -freeze sim:/detector_paridade/im_par 1 0
force -freeze sim:/detector_paridade/parity 1 0
run 10
force -freeze sim:/detector_paridade/im_par 0 0
run 10
run 10
force -freeze sim:/detector_paridade/parity 0 0
run 10
